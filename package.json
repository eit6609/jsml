{
    "name": "@eit6609/jsml",
    "version": "2.0.0",
    "description": "A compact and lossless XML JavaScript representation",
    "keywords": [
        "jsonml",
        "xml",
        "xhtml",
        "html",
        "json",
        "xml2js",
        "js2xml"
    ],
    "Author": "Dario Morandini",
    "scripts": {
        "ci": "ENV=ci eslint src examples/**/code && ENV=ci istanbul cover --root src/ jasmine",
        "cover": "ENV=test istanbul cover --root src/ jasmine JASMINE_CONFIG_PATH=spec/support/jasmine.all.json",
        "lint": "ENV=test eslint src",
        "test": "ENV=test jasmine",
        "test-integration": "ENV=test jasmine JASMINE_CONFIG_PATH=spec/support/jasmine.integration.json"
    },
    "homepage": "https://github.com/eit6609/jsml.git",
    "repository": {
        "type": "git",
        "url": "https://github.com/eit6609/jsml.git"
    },
    "main": "src/index.js",
    "license": "ISC",
    "devDependencies": {
        "eslint": "^5.4.0",
        "istanbul": "^0.4.5",
        "jasmine": "^2.5.2",
        "jasmine-core": "^2.5.2",
        "jasmine-expect": "^3.6.0",
        "jasmine-promises": "^0.4.1",
        "jasmine-spec-reporter": "^3.2.0"
    },
    "dependencies": {
        "htmlparser2": "^4.0.0",
        "iconv-lite": "^0.5.0",
        "lodash": "^4.17.15"
    }
}
